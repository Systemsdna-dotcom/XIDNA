                data: { UserID: userid },
                success: function (data) {
                    var Count = parseInt(data.length);
                    $('.ReminderCount').html(Count);
                    $.each(data, function (i, val) {
                        var Message = val.Message;
                        var MessageLength = parseInt(Message.length);
                        var dots = "......"
                        if (MessageLength > 30) {
                            var message = Message.substring(0, 30) + dots;
                        }
                        else {
                            var message = Message;
                        }
                        $('#ReminderMessage').append('<li><a href="#"><div class="pull-left"><img src="/Content/images/human.png" class="img-circle" alt="User Image"/></div> <h4> Support Team <small><i class="fa fa-clock"></i> 5 mins</small> </h4><p>' + message + '</p></a></li>');
                        $('#NotificationMessages').append('<li> <a href="#"> <i class="fa fa-users text-aqua"></i>' + message + '</a></li>');
                    });


                }
            });
        }
